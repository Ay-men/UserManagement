{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/auth/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/gateway/auth/{everything}",
      "UpstreamHttpMethod": ["GET", "POST", "PUT", "DELETE"],
      "SwaggerKey": "auth",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5053
        }
      ]
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "auth",
      "Config": [
        {
          "Name": "Auth API",
          "Version": "v1",
          "Url": "https://localhost:6053/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "device",
      "Config": [
        {
          "Name": "Devices API",
          "Version": "v1",
          "Url": "https://localhost:7046/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:5051/",
    "ServiceDiscoveryProvider": {
      "Type": "Eureka",
      "Host": "eureka-server",
      "Port": 8761
    }
  }
}
